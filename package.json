{
  "name": "dale-kirsten-wedding-rsvp",
  "version": "2.0.0",
  "description": "Production-grade wedding RSVP system for Dale & Kirsten's wedding",
  "private": true,
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "start": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "populate-guests": "node populate-guests.js",
    "import-csv": "node import-csv-guests.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.39.7",
    "framer-motion": "^12.23.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "wedding",
    "rsvp",
    "react",
    "vite",
    "supabase",
    "typescript"
  ],
  "author": {
    "name": "Dale & Kirsten",
    "email": "kirstendale583@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dale-kirsten/wedding-rsvp.git"
  },
  "homepage": "https://kirstendale.com"
}
